{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "A simple CRUD api for managing todos",
  "main": "index.js",
  "scripts": {
    "start": "node build/app.js",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha spec/** --require babel-core/register --exit",
    "start:dev": "nodemon ./server/app.js --exec babel-node",
    "build": "babel -d ./build ./server -s",
    "config": "cp -Rv ./server/config ./build"
  },
  "keywords": [
    "api",
    "express",
    "node",
    "postgresql"
  ],
  "author": "Abdulsemiu Atanda",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.2.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.6",
    "supertest": "^3.0.0"
  }
}
